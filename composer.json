{
    "name": "dothcom/front-reader",
    "description": "Library laravel to Consume the laranews api",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Dothcom\\FrontReader\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gabriel Novaes",
            "email": "semproblema@gmail.com"
        }
    ],
    "extra" : {
        "laravel": {
            "providers": [
                "Dothcom\\FrontReader\\FrontReaderServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "laravel/pint": "^0.1.2"
    },
    "scripts": {
        "format": [
            "./vendor/bin/pint"
        ],
        "ci": [
            "./vendor/bin/pint --test"
        ]
    }
}
